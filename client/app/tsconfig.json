{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler", // или "node"
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"] 
    },
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": ["src"]
}
